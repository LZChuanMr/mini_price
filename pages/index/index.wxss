/**index.wxss**/
page{
  padding: 20px 10px;
  box-sizing: border-box;
  background: #c9c9c9;
}
.inp_place{
  width: 100%;
  height: 100rpx;
  font-size: 28rpx;
  background: #fff;
  border-radius:10rpx;
  margin-bottom: 5rpx;
  display: flex; 
}
.inp_place view{
  width: 25%;
  padding-left: 25rpx;
  box-sizing: border-box;
  line-height:100rpx;
}
.inp_place input{
  width: 70%;
  height: 100rpx;
  line-height: 100rpx;
}
.place_dj{
  width: 100%;
  height: 100rpx;
  font-size: 28rpx;
  background: #fff;
  border-radius:10rpx;
  margin-bottom: 20rpx;
  display: flex; 
}
.dj_lit{
  width: 25%;
  padding-left: 30rpx;
  box-sizing: border-box;
  line-height:100rpx; 
}
.place_dj input{
  width: 70%;
  height: 100rpx;
  line-height: 100rpx;
}
.tw{
  width: 30%;
  height:50rpx;
  line-height: 50rpx;
  float: right;
  background: white;
}
.tw input{
  width: 30%;
  float: left;
  text-align: center;
}
.tw span{
  display: block;
  width: 20%;
  font-size: 28rpx;
  float: left
}
.ck{
  width: 150px;
  height: 30px;
  line-height: 30px;
  background:#fff;
  color: #4a586a;
  font-weight: bold;
  border-radius: 5px;
  text-align: center;
  position: relative;
  left: 30%;
  margin-top:25%; 
}
.dw{
  width: 12%!important;
}
.inp_ptn{
  width: 100rpx;
  height: 80rpx;
}



.options {
  width: 94%;
  margin: 3%;
  font-size: 0.8rem;
  color: #999;
  overflow: hidden
}
.options text:last-child {
  color: #1268bb;
  float: right;
}
.options .item {
  padding: 0.2rem 0.5rem;
  background-color: #eee;
  float: left !important;
  color: #565656 !important;
  border-radius: 0.1rem;
  margin-bottom: 1%;
  margin-right: 1%;
}

